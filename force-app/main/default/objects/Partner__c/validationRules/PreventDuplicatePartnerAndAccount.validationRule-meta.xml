<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PreventDuplicatePartnerAndAccount</fullName>
    <active>true</active>
    <errorConditionFormula>PartnerName__c =  AccountName__c</errorConditionFormula>
    <errorMessage>Partner Name and Account Name cannot be the same. Please select a different Partner Name.</errorMessage>
</ValidationRule>

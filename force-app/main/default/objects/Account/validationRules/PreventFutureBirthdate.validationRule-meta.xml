<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PreventFutureBirthdate</fullName>
    <active>true</active>
    <errorConditionFormula>Birthdate__c &gt;= TODAY()</errorConditionFormula>
    <errorDisplayField>Birthdate__c</errorDisplayField>
    <errorMessage>Please enter a valid date.</errorMessage>
</ValidationRule>

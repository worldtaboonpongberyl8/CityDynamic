<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IsCanChangeSyncQuote__c</fullName>
    <formula>OR(
   ISPICKVAL( Opportunity__r.StageName, &quot;Open&quot;), 
   ISPICKVAL(Opportunity__r.StageName , &quot;Quotation&quot;),
   ISPICKVAL(Opportunity__r.StageName, &quot;Negotiation&quot;)
 
)</formula>

<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CheckActiveFromBeforeActiveTo</fullName>
    <active>true</active>
    <errorConditionFormula>ActiveFrom__c  &gt;   ActiveTo__c</errorConditionFormula>
    <errorMessage>Active From must be less than Active End. Please correct the dates.</errorMessage>
</ValidationRule>
